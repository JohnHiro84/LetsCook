<h2 class='page-header'>Sign-in</h2>


<form class='form' action="<%= new_api_session_url %>" method="post">
  <input
    type="hidden"
    name="authenticity_token"
    value="<%= form_authenticity_token %>">

  <p>
    <label for='new-user-username'>Email</label>
    <input id='new-user-username' type="text" name="user[username]">
  </p>

  <p>
    <label for='new-user-password'>Password</label>
    <input id='new-user-password' type="password" name="user[password]">
  </p>

  <input type="submit">
</form>
